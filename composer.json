{
  "name": "headsnet/collections",
  "type": "library",
  "description": "Base classes for collections",
  "keywords": ["collection", "php"],
  "homepage": "https://github.com/headsnet/collections",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/headsnet/collections/issues"
  },
  "require": {
    "php": ">=8.1",
    "doctrine/collections": ">1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "phpstan/phpstan": "^1.10",
    "symplify/easy-coding-standard": "^12.1"
  },
  "suggest": {
  },
  "autoload": {
    "psr-4": {
      "Headsnet\\Collections\\": "src/",
      "Headsnet\\Collections\\Test\\": "tests/unit/"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  }
}
